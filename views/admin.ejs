<h1 class="mt-4">Dashboard</h1>
<p class="lead mb-3">Welcome <%= user.name %>
</p>
<a href="/auth/logout" class="btn btn-secondary">Logout</a>
<p>Current Active Sessions:</p>
<% sessionInfo.forEach((session)=>{ %>
    <p></p>
    <ul class="list-group">
        <li class="list-group-item">SessionID: <%= session.sessionId %>
        </li>
        <li class="list-group-item">UserID: <%= session.userId %>
        </li>
        <li class="list-group-item">
            <form action="/admin/<%= session.sessionId %>" method="post">
                <button type="submit" name="revokeSession" class="btn btn-link">Revoke Session</button>
            </form>
        </li>
    </ul>
    <% }) %>